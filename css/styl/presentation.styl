h1
    font-family: Elephant
    font-size: 96px
    background-image: url('../img/panneau.png')
    position: relative;
    display: inline-block;
    margin:0 25px;
    padding:0 10px;
    height: 125px;
    width: 492px;

    :before, :after 
        content:'';
        display: inline-block;
        position:absolute;
        top: 0;
        right: -20px;
        width: 20px;
        height: 100%;
        background:inherit;
        background-position:100% 100%;

    :before
        left:-20px;
        background-position: 0 100%;

